{
  "name": "vue3-bulma-template",
  "version": "0.0.0",
  "scripts": {
    "serve": "npx vite --port 8080",
    "lint": "npx eslint ./src/ --ext .vue,.js,.ts,.cjs,.mjs --fix --ignore-path .gitignore",
    "preview": "npx vite preview --port 8080",
    "typecheck": "npx vue-tsc --noEmit",
    "build": "npx vite build",
    "build:full": "npm run typecheck && npm run vite build",
    "build:gh-pages": "npx vite build --mode github-pages --outDir ./docs",
    "deploy:gh-pages": "npx simple-github-pages-deploy"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "pinia": "^2.0.16",
    "pinia-plugin-persistedstate": "^1.6.3",
    "simpler-fetch": "^7.0.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.0.6",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.2.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vue-tsc": "^0.38.8"
  }
}
